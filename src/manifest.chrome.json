{
    "description": "Decentralised, encrypted chat & collaboration powered by Matrix",
    "manifest_version": 2,
    "name": "Riot",
    "version": "1.5.7.7",
    "content_security_policy": "script-src 'self' 'wasm-eval' 'sha256-zeQRKi6w9P3Pxv1ZMVHvy7glhq8erWzIw4Bd6piw/C8=' 'sha256-1gGaALtpPVJXQMh+bF387CpP/4u0Ol1bYz1SwIVfBR4=' https://www.recaptcha.net https://www.gstatic.com; object-src 'self'",
    "icons": {
        "48": "vector-icons/riot48.png",
        "128": "vector-icons/riot128.png"
    },
    "browser_action": {
        "default_icon": {
            "32": "vector-icons/riot32.png"
        },
        "default_title": "Riot"
    },
    "background": {
        "scripts": ["browser-polyfill.min.js", "background.js"]
    },
    "permissions": ["storage"]
}
